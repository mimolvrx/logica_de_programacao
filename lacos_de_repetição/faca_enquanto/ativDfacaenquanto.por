programa
{
    funcao inicio()
    {
        inteiro numero, quadrado
        
        escreva("Quadrado dos n√∫meros de 15 A 200\n\n")

        numero = 15
       
        faca
        {
            quadrado = numero * numero 
            
            escreva(numero, " * ", numero, " = ", quadrado, "\n")
            
            numero = numero + 1  
        }
        enquanto(numero <= 200)  
    }
}
